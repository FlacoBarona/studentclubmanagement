{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/frontend_app/browser"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Content-Type": "text/html; charset=UTF-8"
        }
      },
      {
        "src": "/(.*\\.js)",
        "dest": "/dist/frontend_app/browser$1",
        "headers": {
          "Content-Type": "application/javascript"
        }
      }
    ]
  }
  